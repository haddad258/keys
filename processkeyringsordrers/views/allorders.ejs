<% include partials/header.ejs %>
<form>
        <!--  <button class="fa fa-arrow-left  btn-success button5" value="GO back" onclick="history.back()"><input type="button" class="fa-arrow-down  btn-success button5" value="Go back!" onclick="history.back()"></button> -->
       <input type="button" class="fa-arrow-down  btn-success button5" value="Go back!" onclick="history.back()">
     </form>
     <p>Reload the list   <a    onClick="window.location.reload();" |  rel="noopener" class="btn btn-info"> Reload <i class="fa fa-refresh"></i>     </a>
     </p>
<h2>  <%= nameid %> </h2>

    <div class="table-wrapper">
        <% if (true) {%>
            <table   class="table table-striped table-striped">
                <thead class="thead-dark">
                    <tr>
                        <th scope="col">keyring id</th>
                        <th scope="col">Label</th>
                        <th scope="col">product id</th>
                        <th scope="col">vehicle id</th>
                        <th scope="col">created</th>
                        <th scope="col">modified</th>
                        <th scope="col">photos</th>
                       
                    </tr>
                </thead>
                <tbody bgcolor="#f68f20">
                    <% orders.forEach((order, index) => { %>
                        <tr>
                            <th scope="row"><%= order.id %></th>
                           <!--  <td><img src="/assets/img/<%= order.image %>" class="rounded-circle order-img" alt=""></td> -->
                           <td><%= order.label %></td>
                           <td><%= order.product_id %></td>
                            <td><%= order.vehicle_id %></td>
                            <td><%= (new Date(parseInt(order.created)*1000)).toJSON().replace("T", " ").substr(0,19) %></td>
                            <td><%= (new Date(parseInt(order.modified)*1000)).toJSON().replace("T", " ").substr(0,19)  %></td>
                            <td><%= order.photos.length %></td>
                            <td>
                                <a href="http://localhost:3000/api/<%= order.id %>"   class="btn btn-sm btn-success">Edit</a>
                                
                                <a href="" class="btn btn-sm btn-danger">Delete</a>
                            </td>
                        </tr>
                    <% }) %>
                </tbody>
            </table>
        <% } else { %>
            <p class="text-center">order not found <a href="/add" >here</a> orders.</p>
        <% } %>
    </div>
</div>
</body>
</html>